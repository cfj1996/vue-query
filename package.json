{
  "name": "zan-vue-query",
  "version": "0.0.1",
  "description": "Hooks for managing, caching and syncing asynchronous and remote data in Vue",
  "author": "Damian Osipiuk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/query.git",
    "directory": "packages/vue-query"
  },
  "homepage": "https://tanstack.com/query",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./build ./coverage",
    "build": "tsup"
  },
  "type": "module",
  "types": "build/index.d.ts",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      }
    },
    "./hook": {
      "import": {
        "types": "./build/hook/index.d.ts",
        "default": "./build/hook/index.js"
      },
      "require": {
        "types": "./build/hook/index.d.cts",
        "default": "./build/hook/index.cjs"
      }
    },
    "./mixin": {
      "import": {
        "types": "./build/minxin/index.d.ts",
        "default": "./build/minxin/index.js"
      },
      "require": {
        "types": "./build/minxin/index.d.cts",
        "default": "./build/minxin/index.cjs"
      }
    },
    "./queryPlugin": {
      "import": {
        "types": "./build/queryPlugin.d.cts",
        "default": "./build/queryPlugin.js"
      },
      "require": {
        "types": "./build/queryPlugin.d.cts",
        "default": "./build/queryPlugin.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "build",
    "src"
  ],
  "nx": {
    "targets": {
      "test:lib": {
        "dependsOn": [
          "test:types"
        ]
      }
    }
  },
  "dependencies": {
    "@vue/composition-api": "1.7.2"
  },
  "devDependencies": {
    "element-ui": "^2.15.14",
    "@types/node": "^20.14.10",
    "@vue/composition-api": "1.7.2",
    "esbuild-plugin-file-path-extensions": "^2.1.2",
    "rimraf": "^5.0.9",
    "tsup": "8.0.2",
    "typescript": "5.3.3",
    "vite": "^4.5.3",
    "vite-plugin-vue2": "2.0.3",
    "vue": "2.6.14",
    "vue-template-compiler": "2.6.14"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.1.2",
    "vue": "^2.6.0 || ^3.3.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
